<div class="row">
  <div class="col-sm-12">
    <div id="ibox_1" class="ibox float-e-margins">
      <div class="ibox-title">
        <h5>Search Result</h5>
        <div class="ibox-tools">
          <a class="collapse-link">
            <i class="fa fa-chevron-up"></i>
          </a>
        </div>
      </div>
      <div class="ibox-content">
        <div class="row">
          <% data.forEach(function(item) { %>
            <div class="col-sm-4">
              <div class="panel panel-primary" style="border-color: <%= color %>">
                <div class="panel-heading" style="background-color: <%= color %>; border-color: <%= color %>;">
                  <strong><%= item.user.name %></strong> &nbsp;&nbsp;&nbsp;  <%= moment(item.created_at).format('Do MMMM, YYYY') %>
                </div>
                <div class="panel-body">
                  <%= item.text %>
                </div>
                <div class="panel-footer">
                  <a href="<%= 'https://twitter.com/' + item.user.screen_name + '/status/' + item.id_str %>" target="_blank">View More on Twitter &nbsp;&nbsp; <i class="fa fa-arrow-right"></i></a>
                </div>
              </div>
            </div>
          <% }); %>
        </div>
      </div>
    </div>
  </div>
  
</div>